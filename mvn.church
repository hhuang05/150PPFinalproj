; Problem 1: Linear Regression
(load "linearalg.js")
(load "common.church")

(define ROWS 500)
(define COLS 2)

(define sigma1 (matrix_multbyScalar (matrix_identity COLS) 2))
(define mu (mvnormal (list 0 0) sigma1))

(define sigma2 (matrix_identity COLS))
(define sigma_prior (wishart sigma2))
(define sigma_priorInv (matrix_invert sigma_prior))

(define (w) (mvnormal mu sigma_priorInv))

(repeat 500 w)
